    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "prettier": "^2.0.4",
    "react": "^16.12.0",
    "react-scripts": "^3.1.0",
    "react-test-renderer": "^16.12.0",
    "rollup": "^2.3.3",
    "rollup-plugin-analyzer": "^3.2.2",
